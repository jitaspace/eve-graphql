{
  "name": "eve-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "concurrently -c \"auto\" --names \"NextJS,GQLGen\" \"next dev\" \"yarn:graphql-codegen:watch\"",
    "build": "yarn graphql-codegen && next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.ts",
    "graphql-codegen:watch": "yarn graphql-codegen --watch",
    "precommit": "lint-staged",
    "prettier:check": "prettier --check '**/*.{ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{ts,tsx}'",
    "test": "yarn prettier:check && yarn link && yarn typecheck && yarn jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@apollo/server": "^4.4.0",
    "@apollo/utils.keyvadapter": "^2.0.0",
    "@as-integrations/next": "^1.2.0",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@eve-graph/esi-apollo-datasource": "^1.12.9",
    "@eve-graph/esi-meta-apollo-datasource": "^0.1.9",
    "@keyv/redis": "^2.5.5",
    "@mantine/carousel": "^5.10.5",
    "@mantine/core": "^5.10.5",
    "@mantine/dates": "^5.10.5",
    "@mantine/dropzone": "^5.10.5",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@mantine/modals": "^5.10.5",
    "@mantine/next": "^5.10.5",
    "@mantine/notifications": "^5.10.5",
    "@mantine/nprogress": "^5.10.5",
    "@mantine/prism": "^5.10.5",
    "@mantine/spotlight": "^5.10.5",
    "@mantine/tiptap": "^5.10.5",
    "@next/bundle-analyzer": "^13.2.1",
    "@tabler/icons": "1.119.0",
    "@tiptap/extension-link": "^2.0.0-beta.218",
    "@tiptap/react": "^2.0.0-beta.218",
    "@tiptap/starter-kit": "^2.0.0-beta.218",
    "@types/node": "^18.16.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server-plugin-response-cache": "^3.8.1",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "embla-carousel-react": "^7.0.9",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "graphql": "^16.6.0",
    "jose": "^4.13.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "next": "13.2.1",
    "next-auth": "^4.19.2",
    "nextjs-cors": "^2.1.2",
    "nextjs-routes": "^1.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.3.0",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/client-preset": "^2.1.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "prettier": "2.8.4",
    "ts-node": "^10.9.1"
  }
}
